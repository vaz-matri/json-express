{
  "name": "@json-express/core",
  "version": "0.1.2",
  "description": "A simple json server which serves all json files",
  "scripts": {
    "dev": "nodemon --watch src src/server.js",
    "serve": "node src/server.js",
    "link-local": "npm link",
    "unlink-local": "npm unlink --global"
  },
  "keywords": [
    "json",
    "server",
    "express",
    "cli"
  ],
  "author": "vazmat",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "express": "5.1.0"
  },
  "devDependencies": {
    "nodemon": "3.1.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vaz-matri/json-express.git"
  },
  "bugs": {
    "url": "https://github.com/vaz-matri/json-express/issues"
  },
  "homepage": "https://jsonexpress.com/",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "bin/",
    "src/"
  ],
  "bin": {
    "json-express": "./bin/exec.js"
  }
}
